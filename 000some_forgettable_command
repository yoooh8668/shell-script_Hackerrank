#multiple comment1
#the space between : and ' is important
: '
SOME_COMMENT1
SOME_COMMENT2
'


#multiple comment2
#this way usual apper no highlight in text editor
: <<'MULTIPLE_COMMENT'
SOME_COMMENT1
SOME_COMMENT2
MULTIPLE_COMMENT


